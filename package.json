{
  "name": "subtuber-monorepo",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:app-main": "npm --workspace=app-main run dev",
    "dev:app-copy": "npm --workspace=app-copy run dev",
    "build:app-main": "npm --workspace=app-main run build",
    "build:app-copy": "npm --workspace=app-copy run build"
  }
}
