{
  "name": "subtuber-monorepo",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:app-main": "npm --workspace=app-main run dev",
    "dev:app-trackdownload": "npm --workspace=app-trackdownload run dev",
    "dev:all": "npm run dev:app-main & npm run dev:app-trackdownload",
    "build:app-main": "npm --workspace=app-main run build",
    "build:app-trackdownload": "npm --workspace=app-trackdownload run build"
  }
}
